<div id="block-block-10" class="block block-block cuadro">
        <h2 class="title"><a href="/countries">Country Data</a></h2>
<script>
function gaFlg(){
ga('send', 'event', 'module', 'linkpage', 'flags');
gtag('event', 'linkpage', {'event_category': 'module','event_label': 'flags'});
}
function shFlgs(id){
gtag('event', 'click', {'event_category': 'button','event_label': 'flags-continent'});
if (id=='world'){
	jQuery('.flg').css({ "display": 'inline-block'});
}else {
jQuery('.flg').css({ "display": 'none'});
jQuery('.flg-'+id).css({ "display": 'inline-block'});
}}
</script>
<div class="text-center"><div class="btn-group" role="group"><button class="btn btn-xs btn-default" onclick="shFlgs('world');">World</button>
<button class="btn btn-xs btn-default" onclick="shFlgs('150');">Europe</button>
